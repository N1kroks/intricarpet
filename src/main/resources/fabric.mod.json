{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",

  "name": "${name}",
  "description": "A carpet extension useful for TNT tech.",
  "authors": [
    "lntricate"
  ],
  "contact": {
    "sources": "https://github.com/lntricate1/intricarpet"
  },

  "icon": "assets/${id}/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "me.lntricate.${id}.IntricarpetMod"
    ],
    "preLaunch": [
      "me.lntricate.${id}.PreLaunch"
    ]
  },
  "mixins": [
    "${id}.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.4",
    "minecraft": "${minecraft_dependency}",
    "carpet": "${carpet_dependency}"
  }
}
